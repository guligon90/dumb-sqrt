{
  "name": "dumb-sqrt",
  "version": "1.0.0",
  "description": "[![thumbnail](thumbnail.png)](http://tsoding.org/dumb-sqrt/)",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "tsc -p tsconfig.json",
    "compose:build": "docker compose build server",
    "compose:start": "docker compose up server",
    "watch": "tsc --watch",
    "update:check": "ncu",
    "update:apply": "ncu -u",
    "update:install": "rimraf node_modules && rm yarn.lock && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsoding/dumb-sqrt.git"
  },
  "author": "me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsoding/dumb-sqrt/issues"
  },
  "homepage": "https://github.com/tsoding/dumb-sqrt#readme",
  "devDependencies": {
    "npm-check-updates": "^16.14.20",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  }
}
